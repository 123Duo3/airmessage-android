<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Pass.DayNight" parent="Base.DayNight">
		<item name="android:windowLightNavigationBar">@bool/is_theme_light</item>
		<item name="android:navigationBarColor">@color/navbarBackground</item>
		<item name="android:navigationBarDividerColor">@color/navbarDivider</item>
	</style>
	
	<style name="Pass.DayNight.NoActionBar" parent="Base.DayNight.NoActionBar">
		<item name="android:windowLightNavigationBar">@bool/is_theme_light</item>
		<item name="android:navigationBarColor">?android:attr/colorBackground</item>
	</style>

	<style name="DayNight.NoActionBar.Protected" parent="@style/DayNight.NoActionBar">
		<!--<item name="android:windowBackground">@color/colorBackgroundClean</item>-->
		<!--<item name="android:navigationBarColor">@color/colorBackgroundClean</item>-->

		<item name="android:windowLightNavigationBar">@bool/is_theme_light</item>
		<item name="android:navigationBarDividerColor">@color/navbarDivider</item>
		<item name="android:navigationBarColor">@color/navbarBackground</item>
	</style>
	
	<style name="Pass.AppTheme.Setup" parent="Base.DayNight.NoActionBar">
		<item name="android:windowLightNavigationBar">@bool/is_theme_light</item>
		<item name="android:navigationBarColor">@color/colorToolbar</item>
	</style>
</resources>